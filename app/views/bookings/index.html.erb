<div class="container mx-auto text-center mt-8 w-1/2 bg-blue-50 rounded-xl p-12 min-h-screen shadow-lg mb-12">
  <h3 class="text-3xl text-gray-900 text-sans font-bold mb-8" >Mes rÃ©servations</h3>
  <% if @bookings.empty? %>
  <p>Aucune rÃ©servation pour le moment. ðŸ˜’</p>
  <% end %>
  <% @bookings.each do |booking| %>
  <div class="max-w-3xl w-full mx-auto z-10">
    <div class="flex flex-col">
      <div class="bg-white border border-white shadow-lg  rounded-3xl p-4 m-4">
        <div class="flex-none sm:flex">
          <div class=" relative h-32 w-32   sm:mb-0 mb-3">
            <%= cl_image_tag booking.humorist.photo.key, :transformation=> [{:height=>400, :width=>400, crop: :fill, :radius=>"max" }] if booking.humorist.photo.attached? %>
          </div>
          <div class="flex-auto sm:ml-5 justify-evenly">
            <div class="flex items-center justify-between sm:mt-2">
              <div class="flex items-center">
                <div class="flex flex-col">
                  <div class="w-full flex-none text-lg text-gray-800 font-bold leading-none"><%= booking.humorist.name %></div>
                  <div class="flex-auto text-gray-500 my-1">
                    <span class="mr-3 "><%= booking.humorist.humor_type %></span><span class="mr-3 border-r border-gray-200  max-h-0"></span><span><%= booking.humorist.address %></span>
                  </div>
                </div>
              </div>
            </div>
            <div class="flex flex-row items-center">
              <div class="flex text-gray-500">
                <p><%= booking.start_date.to_date%> > <%= booking.final_date.to_date %> soit <%= booking.price / booking.humorist.price_per_hour %> jour(s)</p>
              </div>
              <div class="flex-1 inline-flex  hidden items-center">
                <img class="w-5 h-5" src="data:image/svg+xml;base64,">
                <img class="w-5 h-5" src="data:image/svg+xml;base64,">
                <img class="w-5 h-5" src="data:image/svg+xml;base64,">

              </div>
            </div>
            <div class="flex pt-2  text-sm text-gray-500">
              <div class="flex-1 inline-flex items-center">
                <p class=""> <%= booking.price %> euros</p>
              </div>
              <div class="flex-1 inline-flex items-center">
              </div>
              <%= link_to 'RESERVER A NOUVEAU', humorist_path(booking.humorist), class: 'flex-no-shrink bg-green-400 hover:bg-green-500 px-5 ml-4 py-2 text-xs shadow-sm hover:shadow-lg font-medium tracking-wider border-2 border-green-300 hover:border-green-500 text-white rounded-full transition ease-in duration-300'%>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <% end %>
  </div>
</div>
